local atb, in_backpack, hiddenname = "OriginalName"
for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
    if v:IsA("Tool") and v.Name:match("Fruit") and v:GetAttribute(atb) then
        in_backpack = v.Name print(in_backpack)
        hiddenname = v:GetAttribute(atb) print(hiddenname)
    end
end

local CommF_, _Type = game:GetService("ReplicatedStorage").Remotes.CommF_, type or typeof
for i, v in pairs(CommF_:InvokeServer("getInventory")) do
    if _Type(v) == "table" then
        if v.Type == "Blox Fruit" then
            if v.Name == in_backpack then
                print("[Đã có trái " .. in_backpack .. " rồi nên sẽ bỏ qua!]")
                --game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Fruit Checker", Text = "You're already has this fruit.", Duration = 2.5,})
            else
                CommF_:InvokeServer("StoreFruit", hiddenname, game:GetService("Players").LocalPlayer.Backpack:FindFirstChild(in_backpack))
                print("[Đã lưu trữ trái " .. in_backpack .. " ]")
                --game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Fruit Checker", Text = "Stored " .. in_backpack .. " on hand!", Duration = 2.5,})
            end
        end
    end
end
