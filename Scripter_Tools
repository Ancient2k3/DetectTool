local isToggled = false
PlusButton.MouseButton1Click:Connect(function()
    if isToggled then
        tween2:Play()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NganHa2k3/RobloxScript_1/main/UISound"))()
    else
        tween1:Play()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NganHa2k3/RobloxScript_1/main/UISound"))()
    end
    isToggled = not isToggled
end)

